      {"src":"/static/images/icons/android-chrome-512x512.png","sizes":"512x512","type":"image/png"}
    ],
    "splash_pages": null,
    "share_target": {
      "action": "/webshare",
      "params": {
        "text": "score"
      }
    }
  }
